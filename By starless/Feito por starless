    Size = UDim2.fromOffset(560, 350), -- UI Size
    KeySystem = { -- Creates key system
        Key = "AstralTeam", -- key
        Note = "The key (AstralTeam)", -- Note
        URL = "https://github.com/Footagesus/WindUI", -- URL To get key (example: Discord)
        SaveKey = false, -- Saves the key in the folder specified above
    }, 
    Transparent = true,-- UI Transparency
    Theme = "Dark", -- UI Theme
    SideBarWidth = 170, -- UI Sidebar Width (number)
    HasOutline = true, -- Adds Oultines to the window
})
